                        <TextView
                            android:id="@+id/txtSipPassword"
                            style="@style/UIKitTextView.SettingsItemLabel"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
