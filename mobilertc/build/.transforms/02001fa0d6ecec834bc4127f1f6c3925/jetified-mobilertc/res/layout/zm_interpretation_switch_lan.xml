<?xml version="1.0" encoding="utf-8"?>
<com.zipow.videobox.view.ConfInterpretationLanguageBtn xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="44dp"
    android:layout_centerHorizontal="true"
    android:background="@drawable/zm_corner_bg_white_onlight"
    android:gravity="center"
    android:orientation="horizontal"
    android:padding="6dp"
    tools:showIn="@layout/zm_confview_interpretation_switch">

    <TextView
        android:id="@+id/showLanguageImg"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:textSize="12dp"
        style="@style/ZmTextViewIcon"
        android:background="@drawable/zm_lan_bg" />

    <TextView
        android:id="@+id/showLanguageName"
        style="@style/ZMTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="10dp"
        android:singleLine="true"
        android:maxLines="1"
        android:ellipsize="middle"
        android:duplicateParentState="true"
        android:focusable="false" />


</com.zipow.videobox.view.ConfInterpretationLanguageBtn>