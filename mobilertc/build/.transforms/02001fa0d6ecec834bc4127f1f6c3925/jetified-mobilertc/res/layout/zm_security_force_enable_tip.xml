<?xml version="1.0" encoding="utf-8"?>
<us.zoom.androidlib.widget.ZMTip xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/zmForceEnableSecurityPopView"
    android:layout_width="@dimen/zm_security_enable_waitingroom_width"
    android:layout_height="wrap_content"
    android:layout_gravity="right"
    android:orientation="horizontal"
    app:zm_background="@color/zm_white"
    android:layout_marginRight="@dimen/zm_padding_medium">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/zm_padding_medium"
        android:paddingTop="@dimen/zm_padding_medium"
        android:paddingRight="@dimen/zm_padding_medium"
        android:paddingBottom="@dimen/zm_padding_medium"
        android:orientation="horizontal"
        >
    <TextView
        android:id="@+id/zmSecurityTitle"
        style="@style/ZMTextView.Normal.gray.darkMode"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="left"
        android:text="@string/zm_tip_msg_force_enable_waitingroom_166840" />
    <ImageView
        android:id="@+id/zmCloseBtn"
        android:layout_width="@dimen/zm_security_close_btn_width"
        android:layout_height="@dimen/zm_security_close_btn_width"
        android:layout_marginLeft="@dimen/zm_padding_medium_size"
        android:scaleType="center"
        android:src="@drawable/zm_icon_close"
        android:layout_gravity="center_vertical"/>
    </LinearLayout>
</us.zoom.androidlib.widget.ZMTip>